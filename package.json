{
  "name": "@agent-base/webhook-store",
  "version": "0.1.0",
  "description": "Webhook management service with PostgreSQL/pgvector and Google Secret Manager integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:create": "pnpm exec node-pg-migrate create",
    "migrate:up": "pnpm exec node-pg-migrate up",
    "migrate:down": "pnpm exec node-pg-migrate down"
  },
  "keywords": [
    "webhook",
    "pgvector",
    "postgres",
    "typescript",
    "express",
    "google-secret-manager"
  ],
  "author": "Agent Base",
  "license": "MIT",
  "dependencies": {
    "@agent-base/api-client": "^4.0.0",
    "@agent-base/types": "^5.0.0",
    "@google-cloud/secret-manager": "^5.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "pgvector": "^0.1.8",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "node-pg-migrate": "^7.9.1",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}