{
  "name": "@agent-base/webhook-tool",
  "version": "0.3.0",
  "description": "Webhook management service with PostgreSQL/pgvector and Google Secret Manager integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:create": "pnpm exec node-pg-migrate create",
    "migrate:up": "pnpm exec node-pg-migrate up",
    "migrate:down": "pnpm exec node-pg-migrate down",
    "db:clear": "dotenv -- sh -c 'psql \"$DATABASE_URL\" -c \"TRUNCATE TABLE webhook_agent_links, user_webhooks, webhooks CASCADE;\"'",
    "build:docs": "redocly build-docs openapi.json -o ../webhook-tool-web/public/api-docs/index.html"
  },
  "keywords": [
    "webhook",
    "pgvector",
    "postgres",
    "typescript",
    "express",
    "google-secret-manager"
  ],
  "author": "Agent Base",
  "license": "MIT",
  "dependencies": {
    "@agent-base/api-client": "^0.20.0",
    "@agent-base/types": "^0.23.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "pg": "^8.15.5",
    "pgvector": "^0.1.8",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.31",
    "@types/pg": "^8.11.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "node-pg-migrate": "^7.9.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}